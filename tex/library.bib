journal = {The American Naturalist},
keywords = {Animals,Biodiversity,Biological,Competitive Behavior,Environment,Food Chain,Models,Population Dynamics,Recommended by Egbert},
mendeley-tags = {Recommended by Egbert},
number = {2},
pages = {255--66},
pmid = {15278848},
title = {{Large species shifts triggered by small forces.}},
