issn = {1432-9840},
journal = {Ecosystems},
keywords = {Alternative stable states,Chara sp.,Infinite-period bifurcation,Internal eutrophication,Recommended by Egbert,Singular perturbation approach,Slow-fast cycles},
mendeley-tags = {Recommended by Egbert},
month = {feb},
