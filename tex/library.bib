issn = {13541013},
journal = {Global Change Biology},
keywords = {Albedo,Alternative stable states,Atmosphere-vegetation interactions,Climate,Semiarid regions,Turing patterns},
number = {5},
pages = {1104--1112},
title = {{Microscale vegetation-soil feedback boosts hysteresis in a regional vegetation-climate system}},
volume = {14},
year = {2008}
}
