issn = {1054-1500},
journal = {http://oasc12039.247realmedia.com/RealMedia/ads/click{\_}lx.ads/www.aip.org/pt/adcenter/pdfcover{\_}test/L-37/1270473477/x01/AIP-PT/Chaos{\_}ArticleDL{\_}0117/PTBG{\_}Green{\_}1640x440.jpg/434f71374e315a556e61414141774c75?x},
