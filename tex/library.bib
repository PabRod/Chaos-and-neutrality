journal = {PloS one},
keywords = {Chlorophyll,Chlorophyll: metabolism,Eutrophication,Eutrophication: physiology,Fluorescence,North Sea,Particulate Matter,Particulate Matter: analysis,Phytoplankton,Phytoplankton: growth {\&} development,Population Dynamics,Tidal Waves,Time Factors},
