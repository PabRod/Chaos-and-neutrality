journal = {Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
keywords = {WUR,bifurcation,climate change,deglaciation,early warning,thermohaline circulation,tipping point},
mendeley-tags = {WUR},
number = {1962},
pages = {1185--1204},
