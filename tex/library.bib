keywords = {coevolution,complex networks,ecology,mutualism,pollination,seed},
mendeley-tags = {complex networks,ecology},
pages = {567--593},
pmid = {21303086},
title = {{Plant-Animal Mutualistic Networks: The Architecture of Biodiversity}},
