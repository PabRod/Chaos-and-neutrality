month = {jul},
number = {30},
pages = {12227--32},
pmid = {21742979},
publisher = {National Academy of Sciences},
title = {{Aerosol-cloud-precipitation system as a predator-prey problem.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21742979 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3145706},
volume = {108},
